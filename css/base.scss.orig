.icon-items {
    li {
        @include inline-block;
        width: 50%;
        border-top: 1px solid #CDCDCD;

        @media screen and (max-width: $mid-break) {
            width: 100%;

            &.empty {
                display: none;
            }
        }

        @media screen and (min-width: $mid-break) {
            &:nth-child(even) .item-inner {
                border-left: 1px solid #CDCDCD;

                .number {
                    float: right;
                }

                .content {
                    margin-left: 170px;
                    margin-right: 50px;
                }

                .icon {
                    float: left;
                }
            }
        }

        &:nth-child(odd) .item-inner {
            border-right: 1px solid #CDCDCD;
            margin-right: -1px;

            @media screen and (max-width: $mid-break) {
                border-right: none;
                margin-right: 0;
            }
        }


        .item-inner {
            margin-top: 25px;
            margin-bottom: 30px;

            .number {
                border-radius: 100px;
                background-color: $orange;
                width: 30px;
                line-height: 30px;
                text-align: center;
                color: #FEFEFE;
                font-size: 20px;
                font-weight: 600;
                float: left;
                text-shadow: 0 1px 0 #CC5917;
            }

            .content {
                margin-left: 50px;
                margin-right: 170px;
            }

            .icon {
                float: right;
                margin-left: 25px;
                margin-right: 25px;
                width: 109px;
                height: 109px;
            }
        }
    }
}

